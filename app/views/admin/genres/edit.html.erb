<main class="genre_edit_main">
  <div class="genre_edit_main_h3"><h3>ジャンル編集</h3></div>
  <div class="genre_edit_table">
    <%= form_with model:@genre, url: admin_genre_path, local:true do |f| %>
    <table>
      <tr>
        <th>ジャンル</th>
        <td><%= f.text_field :genre_name %></td>
        <td><%= f.submit '変更を保存'%></td>
      </tr>
    </table>
    <% end %>
  </div>
</main>