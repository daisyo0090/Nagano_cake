<div id="admin_login_wrap">
  <div id="admin_login_h2_div">
    <h2>管理者ログイン</h2>
  </div>
  <div id="admin_login_table_div">
    <table>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <tr>
        <th><p>メールアドレス</p></th>
        <td><%= f.email_field :email, autofocus: true, autocomplete: "email", :size=>"40" %></td>
      </tr>
      <tr>
        <th><p>パスワード</p></th>
        <td><%= f.password_field :password, autocomplete: "current-password", :size=>"40" %></td>
      </tr>
    </table>
  </div>
      <p id="admin_login_submit"><%= f.submit "ログイン", class: "btn btn-primary" %></p>
    <% end %>
</div>